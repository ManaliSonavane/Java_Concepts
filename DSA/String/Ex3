//Remove whiteSpaces in a String

class Ex3 {
      public static void main(String args[]){
       String s="Manali Sonavane";
       StringBuilder sb=new StringBuilder();

       for(int i=0;i<s.length();i++){
         if(s.charAt(i)!=' '){
           sb.append(s.charAt(i));
         }
       }
        System.out.print(sb);  
    }
}
