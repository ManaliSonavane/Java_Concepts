//Prime numbers from range 

public class Prime{
     public static void main(String args[]){
        int flag=0; 
        for(int i=2;i<=10;i++){
           for(int j=2;j<=i-1;j++){
               if(i%j==0){
                   flag++;
               }
           }
         if(flag==0){
           System.out.println("Prime number "+ i);
         }
         else{
            flag=0;
         }
        }
   } 
}
