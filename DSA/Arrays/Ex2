//occurrence of a digit in a range

public class Ex2{
    public static void main(String args[]){
        int a[]= {11,12,13,14, 15, 16,17,18,19};
        int temp, count=0, rem;
        for(int i=0;i<a.length;i++){
            temp=a[i];
            while(temp>0){
                rem = temp%10;
                if(rem==1){
                    count++;
                }
                temp=temp/10;
            }
        }
        System.out.print(count);
   }
}
